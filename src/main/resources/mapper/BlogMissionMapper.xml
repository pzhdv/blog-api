<mapper namespace="cn.pzhdv.blog.mapper.BlogMissionMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.pzhdv.blog.entity.BlogMission">
        <id column="mission_id" property="missionId" />
